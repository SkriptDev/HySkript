test "skript-parser-ExprMutableList":
	set {_a::*} to "nine", "three", "four" and "ten"
	set {_a::*} to sorted {_a::*}
	assert 1st element of {_a::*} = "four" with "Four should have been the first after sorting"

	set {_a::*} to reversed {_a::*}
	assert 1st element of {_a::*} = "three" with "Three should have been the first after reversing"
