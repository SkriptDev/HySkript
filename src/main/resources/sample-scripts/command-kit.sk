# Kit Command
# A simple command to give players items from a kit.

player command /kit:
	trigger:
		set {_i::1} to itemstack of 10 of food_kebab_meat
		set {_i::2} to itemstack of tool_hatchet_crude
		set {_i::3} to itemstack of weapon_daggers_crude
		set {_i::4} to itemstack of furniture_crude_bed

		add {_i::*} to inventory of player
